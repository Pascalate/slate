    "/api/Id/driverlicense": {
      "post": {
        "tags": [
          "Id"
        ],
        "summary": "valid Drivers license number. Currently work only for Florida Numbers",
        "parameters": [
          {
            "name": "format",
            "in": "query",
            "description": "",
            "schema": {
              "type": "boolean",
              "description": ""
            }
          }
        ],
        "requestBody": {
          "description": "",
          "content": {
            "application/json-patch+json": {
              "schema": {
                "$ref": "#/components/schemas/DriverLicenseRequest"
              }
            },
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverLicenseRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverLicenseRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DriverLicenseRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Generated Drivers License",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DateRequestType": {
        "enum": [
          "Birthday",
          "Future",
          "Past"
        ],
        "type": "string"
      },
      "UserAgeType": {
        "enum": [
          "Infant",
          "Child",
          "Teenager",
          "Adult",
          "Youth",
          "Retired"
        ],
        "type": "string"
      },
      "CreditCardModel": {
        "type": "object",
        "properties": {
          "issuer": {
            "type": "string",
            "nullable": true
          },
          "number": {
            "type": "string",
            "nullable": true
          },
          "month": {
            "type": "string",
            "nullable": true
          },
          "year": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DriverLicenseRequest": {
        "required": [
          "country",
          "dateOfBirth",
          "firstName",
          "gender",
          "lastName",
          "state"
        ],
        "type": "object",
        "properties": {
          "country": {
            "type": "string",
            "default": "us"
          },
          "state": {
            "type": "string",
            "default": "fl"
          },
          "firstName": {
            "type": "string"
          },
          "middleName": {
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date-time"
          },
          "gender": {
            "type": "string",
            "default": "female"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "tags": [
    {
      "name": "Date",
      "description": "Date Generators"
    },
    {
      "name": "Id",
      "description": "Valid random Identifiers"
    }
  ]
}