{
  "openapi": "3.0.1",
  "info": {
    "title": "Pascalate API Documentation",
    "version": "v1"
  },
  "servers": [
    {
      "url": "http://api.pascalate.com",
      "description": "Pascalate helps App Developers generate random yet valid test Data to test or seed their applications"
    }
  ],
  "paths": {
    "/api/Date": {
      "get": {
        "tags": [
          "Date"
        ],
        "summary": "Dates",
        "description": "sample date remarks",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "description": "Date request type",
            "schema": {
              "$ref": "#/components/schemas/DateRequestType"
            }
          },
          {
            "name": "ageType",
            "in": "query",
            "description": "Desired age type. Used only if type is set to birthday",
            "schema": {
              "$ref": "#/components/schemas/UserAgeType"
            }
          },
          {
            "name": "min",
            "in": "query",
            "description": "Minimum date",
            "schema": {
              "type": "string",
              "description": "Minimum date",
              "nullable": true,
              "example": "12/20/1930"
            }
          },
          {
            "name": "max",
            "in": "query",
            "description": "Maximum date",
            "schema": {
              "type": "string",
              "description": "Maximum date",
              "nullable": true,
              "example": "1/1/2020"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Generated Date",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/api/Finance/creditcard/name": {
      "get": {
        "tags": [
          "Finance"
        ],
        "summary": "Credit Card Name",
        "description": "Returns a random Credit Card Name",
        "responses": {
          "200": {
            "description": "Credit Card Name",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
