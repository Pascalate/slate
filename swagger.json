    "/api/Id/alphanumeric": {
      "get": {
        "tags": [
          "Id"
        ],
        "summary": "Alphanumeric string",
        "parameters": [
          {
            "name": "length",
            "in": "query",
            "description": "length",
            "schema": {
              "type": "integer",
              "description": "length",
              "format": "int32",
              "default": 7,
              "example": 9
            }
          },
          {
            "name": "with_numbers",
            "in": "query",
            "description": "include numbers",
            "schema": {
              "type": "boolean",
              "description": "include numbers",
              "default": true,
