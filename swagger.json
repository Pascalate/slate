              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/api/Finance/creditcard/details": {
      "get": {
        "tags": [
          "Finance"
        ],
        "summary": "Credit Card Data",
        "description": "Generates random Valid Credit Card Details",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "description": "Optional Credit card name",
            "schema": {
              "type": "string",
              "description": "Optional Credit card name",
              "nullable": true,
              "example": "Visa"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Generated Date",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/CreditCardModel"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreditCardModel"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreditCardModel"
                }
              }
            }
          }
        }
      }
    },
    "/api/Id/numeric": {
      "get": {
        "tags": [
          "Id"
        ],
        "summary": "Numerical identifier of a given length",
        "parameters": [
          {
            "name": "length",
            "in": "query",
            "description": "",
            "schema": {
              "type": "integer",
              "description": "",
              "format": "int32",
              "default": 7
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Generated numeric value",
            "content": {
              "text/plain": {
